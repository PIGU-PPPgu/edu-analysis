{
  "name": "figma-frame-faithful-front",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "description": "教育管理系统 - 现代化React应用",
  "author": "Education Management System Team",
  "license": "MIT",
  "homepage": "https://figma-frame-faithful.com",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3000",
    "build": "vite build",
    "build:strict": "tsc && vite build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "build:stats": "npm run build -- --mode=analyze",
    "build:types": "tsc --noEmit",
    "build:desktop": "npm run build && electron-builder",
    "package:desktop": "electron-builder --publish=never",
    "build:with-typecheck": "tsc && vite build",
    "build:dev": "vite build --mode development",
    "preview": "vite preview --port 4173",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "typecheck": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "test": "node src/utils/testing/runner.ts",
    "test:unit": "npm run test -- --filter=unit",
    "test:component": "npm run test -- --filter=component",
    "test:integration": "npm run test -- --filter=integration",
    "test:performance": "npm run test -- --filter=performance",
    "test:e2e": "playwright test",
    "test:e2e:staging": "playwright test --config=playwright.staging.config.ts",
    "test:smoke:production": "playwright test --config=playwright.smoke.config.ts",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "test:ci": "npm run test -- --ci --coverage --watchAll=false",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "quality:check": "npm run format:check && npm run lint:errors-only && npm run type-check:no-emit && npm run test:run",
    "lint:errors-only": "eslint . --quiet",
    "type-check:no-emit": "tsc --noEmit --skipLibCheck",
    "quality:fix": "npm run format && npm run lint:fix",
    "prepare": "husky",
    "dev:no-hmr": "vite --config vite.no-hmr.config.ts",
    "db:init": "node reinitialize-database.js",
    "db:reset": "echo 'Setting up database with user isolation...' && node reinitialize-database.js",
    "db:migrate:test": "echo 'Running test database migrations...'",
    "db:seed:test": "echo 'Seeding test database...'",
    "python:deps": "cd python-data-processor && pip install -r requirements.txt",
    "python:start": "cd python-data-processor && python app.py",
    "setup:auth": "npm run db:reset && npm run python:deps",
    "docker:build": "docker build -f docker/Dockerfile.production -t figma-frame-faithful:latest .",
    "docker:run": "docker run -p 80:80 figma-frame-faithful:latest",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:rebuild": "docker-compose down && docker-compose build --no-cache && docker-compose up -d",
    "docker:logs": "docker-compose logs -f",
    "docker:logs:frontend": "docker-compose logs -f frontend-dev",
    "docker:logs:backend": "docker-compose logs -f backend-api",
    "docker:logs:python": "docker-compose logs -f python-service",
    "docker:health": "chmod +x scripts/docker-health-check.sh && ./scripts/docker-health-check.sh",
    "docker:clean": "docker-compose down -v && docker system prune -f",
    "docker:dev": "docker-compose up --build",
    "k8s:deploy:staging": "kubectl apply -f k8s/staging/",
    "k8s:deploy:production": "kubectl apply -f k8s/production/",
    "deploy:staging": "./scripts/deploy-staging.sh",
    "deploy:production": "./scripts/deploy-production.sh",
    "health:check": "curl -f http://localhost:3000/health || exit 1",
    "security:audit": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix",
    "monitoring:deploy": "./scripts/deploy-monitoring.sh",
    "monitoring:deploy:monitoring-only": "./scripts/deploy-monitoring.sh --monitoring-only",
    "monitoring:deploy:logging-only": "./scripts/deploy-monitoring.sh --logging-only",
    "monitoring:prometheus:port-forward": "kubectl port-forward -n monitoring svc/prometheus 9090:9090",
    "monitoring:grafana:port-forward": "kubectl port-forward -n monitoring svc/grafana 3000:3000",
    "monitoring:kibana:port-forward": "kubectl port-forward -n logging svc/kibana 5601:5601",
    "monitoring:alertmanager:port-forward": "kubectl port-forward -n monitoring svc/alertmanager 9093:9093",
    "monitoring:status": "kubectl get pods -n monitoring && kubectl get pods -n logging",
    "backup:deploy": "./scripts/backup-restore.sh deploy",
    "backup:status": "./scripts/backup-restore.sh status",
    "backup:database": "./scripts/backup-restore.sh backup database",
    "backup:config": "./scripts/backup-restore.sh backup config",
    "backup:all": "./scripts/backup-restore.sh backup all",
    "backup:list": "./scripts/backup-restore.sh list",
    "backup:cleanup": "./scripts/backup-restore.sh cleanup",
    "restore:database": "./scripts/backup-restore.sh restore database",
    "restore:config": "./scripts/backup-restore.sh restore config"
  },
